<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="service"
            type="com.example.cristian.myapplication.data.models.Servicio"/>
        <import type="android.view.View"/>
    </data>

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <TextView
            android:id="@+id/fechaMontaTxt"
            style="@style/textview_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="16dp"
            android:text="Fecha Monta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fechaMonta"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/fechaMontaTxt"
            app:layout_constraintTop_toBottomOf="@+id/fechaMontaTxt"
            tools:text="20/02/2018" />

        <TextView
            android:id="@+id/fechaPalpacionTxt"
            style="@style/textview_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="24dp"
            android:layout_marginTop="16dp"
            android:text="Fecha de Palpación"
            app:layout_constraintStart_toEndOf="@+id/fechaNacimientoTxt"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/fechaPalpacion"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/fechaPalpacionTxt"
            app:layout_constraintTop_toBottomOf="@+id/fechaPalpacionTxt"
            tools:text="20/02/2018" />

        <TextView
            android:id="@+id/fechaNacimientoTxt"
            style="@style/textview_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="32dp"
            android:layout_marginTop="13dp"
            android:text="Fecha Nacimiento"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fechaMonta" />

        <TextView
            android:id="@+id/fechaNacimiento"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/fechaNacimientoTxt"
            app:layout_constraintTop_toBottomOf="@+id/fechaNacimientoTxt"
            tools:text="20/02/2018" />

        <TextView
            android:id="@+id/propositoTxt"
            style="@style/textview_label"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Propósito"
            app:layout_constraintStart_toStartOf="@+id/fechaPalpacionTxt"
            tools:layout_editor_absoluteY="64dp" />

        <TextView
            android:id="@+id/proposito"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="@+id/propositoTxt"
            app:layout_constraintTop_toBottomOf="@+id/propositoTxt"
            tools:text="Leche" />
    </android.support.constraint.ConstraintLayout>
</layout>
